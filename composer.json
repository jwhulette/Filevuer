{
    "name": "jwhulette/filevuer",
    "description": "A remote FTP & S3 file manager using VueJS",
    "keywords": [
        "Laravel",
        "S3",
        "FTP"
    ],
    "homepage": "https://github.com/jwhulette/Filevuer",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Wes Hulette",
            "email": "jwhulette@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "aws/aws-sdk-php": "^3.0",
        "league/flysystem": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maennchen/zipstream-php": "^2.0",
        "spatie/laravel-package-tools": "^1.4"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.9",
        "codedungeon/phpunit-result-printer": "^0.30",
        "mikey179/vfsstream": "^1.6",
        "nunomaduro/larastan": "^0.7.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Jwhulette\\Filevuer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jwhulette\\Filevuer\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "t": "vendor/bin/phpunit --color=always",
        "tf": "vendor/bin/phpunit --color=always --filter ",
        "suite": [
            "vendor/bin/phpstan analyse --memory-limit=2G",
            "vendor/bin/phpunit --color=always"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jwhulette\\Filevuer\\FilevuerServiceProvider"
            ]
        }
    }
}
